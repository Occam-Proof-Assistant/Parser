{
  "name": "parser",
  "author": "James Smith",
  "version": "0.0.1",
  "license": "MIT",
  "homepage": "https://github.com/occam-proof-assistant/Parser",
  "description": "The Occam proof assistant's parser.",
  "repository": {
    "type": "git",
    "url": "https://github.com/occam-proof-assistant/Parser"
  },
  "dependencies": {
    "easyui": "^3.6.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "mocha": "^3.0.2",
    "npm-update-outdated": "^0.1.6",
    "watch": "^0.19.2"
  },
  "scripts": {
    "clean:lib": "rm -rf ./lib/*",
    "clean:example": "rm -rf ./example/lib/* rm ./example.js",
    "clean": "npm run clean:lib && npm run clean:example",
    "babel:lib": "babel ./es6 --presets=es2015 --out-dir=./lib",
    "babel:example": "babel ./example/es6 --presets es2015 --out-dir ./example/lib",
    "browserify:example": "browserify ./example/index.js --standalone example > ./example.js",
    "babel:lib-debug": "babel ./es6 --presets=es2015 --source-maps inline --out-dir=./lib",
    "babel:example-debug": "babel ./example/es6 --presets es2015 --source-maps inline --out-dir ./example/lib",
    "browserify:example-debug": "browserify ./example/index.js --standalone example --debug > ./example.js",
    "babel": "npm run babel:lib && npm run babel:example",
    "browserify": "npm run browserify:example",
    "build": "npm run clean && npm run babel && npm run browserify",
    "watch": "watch 'npm run build' ./es6 ./example/es6",
    "babel-debug": "npm run babel:lib-debug && npm run babel:example-debug",
    "browserify-debug": "npm run browserify:example-debug",
    "build-debug": "npm run clean && npm run babel-debug && npm run browserify-debug",
    "watch-debug": "watch 'npm run build-debug' ./es6 ./example/es6",
    "update-outdated": "npm-update-outdated"
  }
}
